{"components/brand/show-more":"{{#if brand.facets}}\r\n    {{> components/faceted-search/show-more-facets brand}}\r\n{{/if}}","components/faceted-search/show-more-facets":"<div class=\"modal-body\">\r\n    <h1 class=\"facet-quick-heading modal-parent-name\">\r\n        {{all_facets}}\r\n    </h1>\r\n    <div class=\"form-field\" id=\"facetedSearch-filterItems\">\r\n        <input class=\"form-input\" type=\"search\" placeholder={{lang 'forms.search.search'}}>\r\n    </div>\r\n    <ul class=\"navList facetedSearch-optionColumns\">\r\n        {{#each facets}}\r\n            {{#if facet '===' ../all_facets}}\r\n                {{#each items}}\r\n                    <li class=\"navList-item {{#if selected }}selected{{/if}}\"><a\r\n                        href=\"{{url}}\"\r\n                        class=\"navList-action modal-link{{#if selected}} is-active {{/if}} {{#if selected}} is-selected {{/if}}\"\r\n                        data-id=\"{{ id }}\"\r\n                        data-faceted-search-facet>{{title}} ({{count}})</a></li>\r\n                {{/each}}\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n</div>"}
