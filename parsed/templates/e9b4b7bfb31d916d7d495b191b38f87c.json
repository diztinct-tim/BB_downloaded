{"components/faceted-search/selected-facets":"<!-- <div class=\"facetedSearch-refineFilters sidebarBlock\"> -->\r\n    \r\n\r\n    {{#if this.items}}\r\n    <div class=\"facetedSearch-refineFilters sidebarBlock\">\r\n        <p class=\"you-selected\">You've selected:</p>\r\n        <ul class=\"inlineList inlineList--labels\">\r\n            {{#each this.items}}\r\n                <li>Filter:\r\n                    <a\r\n                        href=\"{{ url }}\"\r\n                        class=\"facetLabel\"\r\n                        data-faceted-search-facet>\r\n                        \r\n                            {{ title }}\r\n\r\n                        <svg class=\"icon\">\r\n                            <use xlink:href=\"#icon-close\" />\r\n                        </svg>\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <a href=\"{{ remove_all_url }}\" data-faceted-search-facet>Clear</a>\r\n    </div>\r\n    {{/if}}\r\n    \r\n\r\n    {{#if remove_all_url}}\r\n        <!-- <a href=\"{{ remove_all_url }}\" data-faceted-search-facet>Clear</a> -->\r\n    {{/if}}\r\n\r\n"}
