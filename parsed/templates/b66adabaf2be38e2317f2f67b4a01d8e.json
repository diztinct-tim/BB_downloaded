{"components/cart/preview":"<div class=\"modal-header\">\r\n    <h2 class=\"modal-header-title\">\r\n        {{lang 'cart.added_to_cart.what_next' num_products=cart.quantity}}\r\n    </h2>\r\n</div>\r\n\r\n<div class=\"modal-body\">\r\n    <div class=\"previewCart\">\r\n        <section class=\"previewCartCheckout\">\r\n            {{#if cart.show_primary_checkout_button}}\r\n                <a href=\"{{urls.checkout.single_address}}\" class=\"button button--primary\">\r\n                    {{lang 'cart.added_to_cart.proceed_to_checkout'}}\r\n                </a>\r\n            {{/if}}\r\n\r\n            <div class=\"previewCartCheckout-additionalCheckoutButtons\">\r\n                {{#each cart.additional_checkout_buttons}}\r\n                    {{{this}}}\r\n                {{/each}}\r\n            </div>\r\n\r\n            <div class=\"previewCartCheckout-subtotal\">\r\n                {{lang 'cart.added_to_cart.order_subtotal'}}\r\n\r\n                <strong class=\"previewCartCheckout-price\">\r\n                    {{cart.sub_total.formatted}}\r\n                </strong>\r\n            </div>\r\n\r\n            <p data-cart-quantity=\"{{cart.quantity}}\">\r\n                {{lang 'cart.added_to_cart.your_cart_contains' num_products=cart.quantity}}\r\n            </p>\r\n\r\n            <a href=\"#\" class=\"button button--primary\" role=\"button\" data-reveal-close>\r\n                {{lang 'cart.added_to_cart.continue_shopping'}}\r\n            </a>\r\n\r\n            <a href=\"{{urls.cart}}\" class=\"button\">\r\n                {{lang 'cart.added_to_cart.view_or_edit_cart'}}\r\n            </a>\r\n        </section>\r\n        <section class=\"productView\">\r\n            {{#with cart.added_item}}\r\n                <figure class=\"productView-image\">\r\n                    <img src=\"{{getImage image 'product' (cdn ../theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\" />\r\n                </figure>\r\n\r\n                <div class=\"productView-details\">\r\n                    <h4 class=\"productView-title\">\r\n                        {{name}}\r\n                    </h4>\r\n\r\n                    <div class=\"productView-brand\">\r\n                        {{brand.name}}\r\n                    </div>\r\n\r\n                    <div class=\"productView-price\">\r\n                        {{quantity}} &times; {{price.formatted}}\r\n                    </div>\r\n\r\n                    {{#each options}}\r\n                        <dl class=\"productView-info\">\r\n                            <dt class=\"productView-info-name\">\r\n                                {{name}}\r\n                            </dt>\r\n                            <dd class=\"productView-info-value\">\r\n                                {{value}}\r\n                            </dd>\r\n                        </dl>\r\n                    {{/each}}\r\n                </div>\r\n            {{/with}}\r\n        </section>\r\n    </div>\r\n\r\n    {{#if cart.suggested_products}}\r\n        <section class=\"suggestiveCart\">\r\n            <h4>\r\n                {{lang 'cart.added_to_cart.you_might_also_like'}}&hellip;\r\n            </h4>\r\n\r\n            <ul class=\"productGrid\">\r\n                {{#each cart.suggested_products}}\r\n                    <li class=\"product\">\r\n                        {{> components/products/card hide_product_quick_view=true theme_settings=../theme_settings}}\r\n                    </li>\r\n                {{/each}}\r\n            </ul>\r\n        </section>\r\n    {{/if}}\r\n</div>\r\n","components/products/card":"<article class=\"card {{#if alternate}}card--alternate{{/if}}\">\r\n    <figure class=\"card-figure\">\r\n        <a href=\"{{url}}\">\r\n            <img class=\"card-image\" src=\"{{getImage image 'gallery' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\">\r\n        </a>\r\n        <figcaption class=\"card-figcaption\">\r\n            <div class=\"card-figcaption-body\">\r\n                \r\n                {{#if show_cart_action}}\r\n\r\n                    {{#if has_options}}\r\n                        <a href=\"{{url}}\" class=\"button button--small card-figcaption-button\" data-product-id=\"{{id}}\">VIEW OPTIONS</a>\r\n                    {{/if}}\r\n\r\n                    {{#if add_to_cart_url }}\r\n                        <a href=\"{{add_to_cart_url}}\" class=\"button button--small card-figcaption-button\">ADD TO BAG</a>\r\n                    {{/if}}\r\n\r\n                    {{#if out_of_stock_message }}\r\n                        <a href=\"{{url}}\" class=\"button button--small card-figcaption-button\" data-product-id=\"{{id}}\">OUT OF STOCK</a>\r\n                    {{/if}} \r\n\r\n                {{/if}}\r\n\r\n            </div>\r\n        </figcaption>\r\n    </figure>\r\n    <div class=\"card-body\">\r\n        \r\n        <p class=\"card-text brand-name\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n        \r\n        <h4 class=\"card-title\">\r\n            <a href=\"{{url}}\">{{name}}</a>\r\n        </h4>\r\n        <div class=\"card-text\" data-test-info-type=\"price\">\r\n            {{> components/products/price price=price}}\r\n        </div>\r\n\r\n        {{#if custom_fields}}\r\n            <ul>\r\n            {{#each custom_fields}}\r\n                <li class=\"custom-field {{name}}\">\r\n                    <span class=\"productView-info-name {{name}}\">{{name}}:</span>\r\n                    <span class=\"productView-info-value {{{value}}}\">{{{value}}}</span>\r\n                </li>\r\n            {{/each}}\r\n            </ul>    \r\n        {{/if}}\r\n\r\n        <a href=\"{{url}}\" class=\"po-wrap\">\r\n            <div class=\"yotpo bottomLine\"\r\n            data-product-id=\"{{id}}\"\r\n            data-url=\"{{url}}\">\r\n            </div>\r\n        </a>\r\n        \r\n    </div>\r\n</article>\r\n","components/products/price":"<!-- {{#if price.with_tax}}\r\n    <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n        {{#if price.rrp_with_tax}}\r\n            <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n        {{#if price.without_tax}}\r\n            {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n        {{/if}}\r\n    </div>\r\n{{/if}}\r\n\r\n{{#if price.without_tax}} -->\r\n    <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n\r\n        {{#if schema_org}}\r\n            <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n            <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n        {{/if}}\r\n        <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n        {{#if price.with_tax}}\r\n            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n        {{/if}}\r\n        {{#if price.rrp_without_tax}}\r\n            <span data-product-rrp-without-tax class=\"price price--rrp retail\">{{price.rrp_without_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span class=\"percent-off\"></span>\r\n    </div>\r\n<!-- {{/if}} -->\r\n\r\n<!-- {{#if page_type '===' 'product'}}\r\n    {{#if price.saved}}\r\n        <div class=\"price-section price-section--saving\">\r\n            <span class=\"price\">\r\n                {{lang 'products.you_save' amount=price.saved.formatted}}\r\n            </span>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n -->"}
