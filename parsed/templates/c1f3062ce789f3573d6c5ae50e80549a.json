{"components/common/alert-error":"<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/alert-success":"<div class=\"alertBox alertBox--success\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n","components/common/share":"{{#if settings.add_this.buttons}}\r\n    <div class=\"addthis_toolbox addthis_32x32_style\" addthis:url=\"{{url}}\"\r\n         addthis:title=\"{{title}}\">\r\n        <ul class=\"socialLinks\">\r\n            {{#each settings.add_this.buttons}}\r\n                <li class=\"socialLinks-item socialLinks-item--{{service}}\">\r\n                    <a class=\"addthis_button_{{service}} icon icon--{{service}}\" {{{annotation}}}>\r\n                        \r\n                        {{#if service '===' 'facebook'}}\r\n                            <svg>\r\n                                <use xlink:href=\"#icon-facebook\"/>\r\n                            </svg>\r\n                        {{/if}}\r\n\r\n                        {{#if service '===' 'email'}}\r\n                            <svg>\r\n                                <use xlink:href=\"#icon-envelope\"/>\r\n                            </svg>\r\n                        {{/if}}\r\n\r\n                        {{#if service '===' 'print'}}\r\n                            <svg>\r\n                                <use xlink:href=\"#icon-print\"/>\r\n                            </svg>\r\n                        {{/if}}\r\n\r\n                        {{#if service '===' 'twitter'}}\r\n                            <svg>\r\n                                <use xlink:href=\"#icon-twitter\"/>\r\n                            </svg>\r\n                        {{/if}}\r\n\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <script type=\"text/javascript\"\r\n                src=\"//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-4e94ed470ee51e32\"></script>\r\n        <script>\r\n            if (typeof(addthis) === \"object\") {\r\n                addthis.toolbox('.addthis_toolbox');\r\n            }\r\n        </script>\r\n    </div>\r\n{{/if}}\r\n","components/products/customizations/checkbox":"<div class=\"form-field\">\r\n    <input class=\"form-checkbox\" type=\"checkbox\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\" {{#if value}}checked{{/if}}>\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\r\n        {{ this.display_name }}\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n</div>\r\n","components/products/customizations/file":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\r\n        {{ this.display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input class=\"form-file\" type=\"file\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\">\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n</div>\r\n","components/products/customizations/select":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{ display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select class=\"form-select form-select--small\" name=\"ProductFields[{{id}}]\" id=\"customization_{{id}}_{{../id}}\"{{#if required}} required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_an_option'}}</option>\r\n        {{#each values}}\r\n            <option value=\"{{this}}\" {{#if ../value '===' this}}selected{{/if}}>{{this}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n","components/products/customizations/text":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{ display_name }}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\" value=\"{{value}}\">\r\n</div>\r\n","components/products/customizations/textarea":"<div class=\"form-field\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\r\n        {{display_name}}:\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <textarea class=\"form-input\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\">{{value}}</textarea>\r\n</div>\r\n","components/products/options/date":"<div class=\"form-field\" data-product-attribute=\"date\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\r\n        {{this.display_name}}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.month'}}</option>\r\n        {{#for 1 12}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{lang (concat 'common.short_months.' $index)}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.day'}}</option>\r\n        {{#for 1 31}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n    <select class=\"form-select form-select--date\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'common.year'}}</option>\r\n        {{#for earliest_year latest_year}}\r\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\r\n                {{$index}}\r\n            </option>\r\n        {{/for}}\r\n    </select>\r\n</div>\r\n","components/products/options/input-checkbox":"<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n\r\n<!--         {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    <input \r\n        class=\"form-checkbox\"\r\n        type=\"checkbox\"\r\n        name=\"attribute[{{id}}]\"\r\n        id=\"attribute-{{id}}\"\r\n        value=\"{{value}}\"\r\n        {{#if checked}}checked{{/if}}\r\n        {{#if required}}required{{/if}}>\r\n\r\n    <label class=\"form-label {{class}}\" for=\"attribute-{{id}}\">{{label}}</label>\r\n</div>\r\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n<!--         {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    <input class=\"form-file\" type=\"file\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\r\n    {{#if original_name}}\r\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\r\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\r\n    {{/if}}\r\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\r\n\r\n</div>\r\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n<!--         {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    <input class=\"form-input\" type=\"number\" id=\"attribute_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n","components/products/options/input-text":"<div class=\"form-field\" data-product-attribute=\"input-text\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\r\n</div>\r\n","components/products/options/product-list":"<div class=\"form-field\" data-product-attribute=\"product-list\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'products.select_one'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#if values.0.image}}\r\n        <ul class=\"productOptions-list\">\r\n            {{#unless required}}\r\n                <li class=\"productOptions-list-item\">\r\n                    <input class=\"form-radio\"\r\n                           type=\"radio\"\r\n                           name=\"attribute[{{id}}]\"\r\n                           value=\"0\"\r\n                           id=\"attribute_0_{{id}}\"\r\n                           {{#if defaultValue '==' 0}}checked{{/if}} required>\r\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\r\n                </li>\r\n            {{/unless}}\r\n            {{#each values}}\r\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\r\n                    {{#if image}}\r\n                        <figure class=\"productOptions-list-item-figure\">\r\n                            <img class=\"productOptions-list-item-image\" src=\"{{getImage image 'thumb'}}\" alt=\"{{image.alt}}\">\r\n                        </figure>\r\n                    {{/if}}\r\n                    <div class=\"productOptions-list-item-content\">\r\n                        <input\r\n                            class=\"form-radio\"\r\n                            type=\"radio\"\r\n                            name=\"attribute[{{../id}}]\"\r\n                            value=\"{{id}}\"\r\n                            id=\"attribute_{{id}}\"\r\n                            {{#if selected}}checked{{/if}}\r\n                            {{#if ../required}}required{{/if}}>\r\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    {{else}}\r\n        <input class=\"form-radio\"\r\n               type=\"radio\"\r\n               name=\"attribute[{{id}}]\"\r\n               value=\"0\"\r\n               id=\"attribute_0_{{id}}\"\r\n               {{#if defaultValue '==' 0}}checked{{/if}}\r\n               {{#if required}}required{{/if}}>\r\n        <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\r\n        {{#each values}}\r\n            <input\r\n                class=\"form-radio\"\r\n                type=\"radio\"\r\n                name=\"attribute[{{../id}}]\"\r\n                value=\"{{id}}\"\r\n                id=\"attribute_{{id}}\"\r\n                {{#if selected}}checked{{/if}}\r\n                {{#if ../required}}required{{/if}}>\r\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{ this.display_name }}:\r\n\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/set-rectangle":"<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        <!-- {{#if required}}\r\n            <small>{{lang 'products.select_one'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n\r\n        <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{ this.display_name }}\r\n\r\n<!--         {{#if required}}\r\n            <small>{{lang 'products.select_one'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option>Select an option</option>\r\n        {{#each this.values}}\r\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>\r\n","components/products/options/swatch":"<div class=\"form-field\" data-product-attribute=\"swatch\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n\r\n        <!-- {{#if required}}\r\n            <small>{{lang 'products.select_one'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\r\n        <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            {{#if image}}\r\n                <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"thumb\"}}');\"></span>\r\n            {{/if}}\r\n            {{#if data.[2]}}\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\r\n            {{else}}\r\n                {{#if data.[1]}}\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\r\n                {{else}}\r\n                    {{#if data.[0]}}\r\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n","components/products/options/textarea":"<div class=\"form-field\" data-product-attribute=\"textarea\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{display_name}}:\r\n\r\n<!--         {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}} -->\r\n    </label>\r\n\r\n    <textarea class=\"form-input\" id=\"attribute_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>\r\n        {{prefill}}\r\n    </textarea>\r\n</div>\r\n","components/products/price":"<!-- {{#if price.with_tax}}\r\n    <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n        {{#if price.rrp_with_tax}}\r\n            <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n        {{#if price.without_tax}}\r\n            {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n        {{/if}}\r\n    </div>\r\n{{/if}}\r\n\r\n{{#if price.without_tax}} -->\r\n    <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n\r\n        {{#if schema_org}}\r\n            <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n            <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n        {{/if}}\r\n        <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n        {{#if price.with_tax}}\r\n            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n        {{/if}}\r\n        {{#if price.rrp_without_tax}}\r\n            <span data-product-rrp-without-tax class=\"price price--rrp retail\">{{price.rrp_without_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span class=\"percent-off\"></span>\r\n    </div>\r\n<!-- {{/if}} -->\r\n\r\n<!-- {{#if page_type '===' 'product'}}\r\n    {{#if price.saved}}\r\n        <div class=\"price-section price-section--saving\">\r\n            <span class=\"price\">\r\n                {{lang 'products.you_save' amount=price.saved.formatted}}\r\n            </span>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n -->","components/products/product-view":"<div class=\"productView\">\r\n    {{#each product.reviews.messages}}\r\n        {{#if error}}\r\n            {{>components/common/alert-error error}}\r\n        {{/if}}\r\n        {{#if success}}\r\n            {{>components/common/alert-success success}}\r\n        {{/if}}\r\n    {{/each}}\r\n\r\n    \r\n\r\n    <section class=\"productView-images\" data-image-gallery>\r\n        <figure class=\"productView-image\"\r\n                data-image-gallery-main\r\n                data-zoom-image=\"{{getImage product.main_image 'zoom' (cdn theme_settings.default_image_product)}}\"\r\n                >\r\n            <a href=\"{{getImage product.main_image 'zoom' (cdn theme_settings.default_image_product)}}\">\r\n                <img src=\"{{getImage product.main_image 'product' (cdn theme_settings.default_image_product)}}\" alt=\"{{product.main_image.alt}}\" data-main-image>\r\n            </a>\r\n        </figure>\r\n        <ul class=\"productView-thumbnails\">\r\n            {{#each product.images}}\r\n                <li class=\"productView-thumbnail\">\r\n                    <a\r\n                        class=\"productView-thumbnail-link\"\r\n                        href=\"{{getImage this 'product' (cdn ../theme_settings.default_image_product)}}\"\r\n                        data-image-gallery-item\r\n                        data-image-gallery-new-image-url=\"{{getImage this 'product' (cdn ../theme_settings.default_image_product)}}\"\r\n                        data-image-gallery-zoom-image-url=\"{{getImage this 'zoom' (cdn ../theme_settings.default_image_product)}}\">\r\n                        <img src=\"{{getImage this 'thumb' (cdn ../theme_settings.default_image_product)}}\" alt=\"{{this.alt}}\">\r\n                    </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n    </section>\r\n\r\n\r\n    <section class=\"productView-details\">\r\n        <div class=\"productView-product\">\r\n\r\n            <h2 class=\"productView-brand\"{{#if schema}} itemprop=\"brand\" itemscope itemtype=\"http://schema.org/Brand\"{{/if}}>\r\n                <a href=\"{{product.brand.url}}\"{{#if schema}} itemprop=\"url\"{{/if}}><span{{#if schema}} itemprop=\"name\"{{/if}}>{{product.brand.name}}</span></a>\r\n            </h2>\r\n\r\n            <h1 class=\"productView-title ProductPageHeading\" {{#if schema}}itemprop=\"name\"{{/if}}>{{product.title}}</h1>\r\n\r\n\r\n            <!-- PRACTICAL DATA REVIEWS -->\r\n            <!-- <div class=\"pdProductReviewStarsSummary\" productId=\"{{product.id}}\"></div> -->\r\n\r\n\r\n<!--             <script type=\"text/javascript\">\r\n            (function e(){ if(typeof yotpo==='undefined') { var e=document.createElement(\"script\");e.type=\"text/javascript\",e.async=true,e.src=\"//staticw2.yotpo.com/kj8REvvTdnXvtBjLDKBFI8bToUW1wpAm6N3S6fj5/widget.js\";var t=document.getElementsByTagName(\"script\")[0];t.parentNode.insertBefore(e,t) } else { yotpo.initialized=false;yotpo.init(); } })();\r\n            </script>\r\n            <span class=\"po-wrap pdp-po\">\r\n                <div class=\"yotpo bottomLine\"\r\n                data-product-id=\"{{product.id}}\"\r\n                data-url=\"{{product.url}}\">\r\n                </div>\r\n            </span> -->\r\n            <span class=\"rating-placeholder\"></span>\r\n\r\n\r\n            <div class=\"productView-price\">\r\n                {{> components/products/price price=product.price schema_org=schema}}\r\n\r\n                <span class=\"free-shipping-over\">FREE SHIPPING OVER $49</span>\r\n            </div>\r\n\r\n            {{product.detail_messages}}\r\n            <dl class=\"productView-info\">\r\n\r\n                {{#if product.availability}}\r\n                    <dt class=\"productView-info-name\">{{lang 'products.availability'}}</dt>\r\n                    <dd class=\"productView-info-value\">{{product.availability}}</dd>\r\n                {{/if}}\r\n\r\n                {{#if product.shipping}}\r\n                    {{#if product.shipping.calculated}}\r\n                        <dt class=\"productView-info-name\">{{lang 'products.shipping'}}</dt>\r\n                        <dd class=\"productView-info-value\">{{lang 'products.shipping_calculated'}}</dd>\r\n                    {{else}}\r\n                        {{#if product.shipping.price.value '===' 0}}\r\n                            <dt class=\"productView-info-name\">{{lang 'products.shipping'}}</dt>\r\n                            <dd class=\"productView-info-value\">{{lang 'products.shipping_free'}}</dd>\r\n                        {{else}}\r\n                            <dt class=\"productView-info-name\">{{lang 'products.shipping'}}</dt>\r\n                            <dd class=\"productView-info-value\">{{lang 'products.shipping_fixed' amount=product.shipping.price.formatted}}</dd>\r\n                        {{/if}}\r\n                    {{/if}}\r\n                {{/if}}\r\n                \r\n            </dl>\r\n        </div>\r\n    </section>\r\n\r\n\r\n\r\n    <section class=\"productView-details\">\r\n        <div class=\"call-to-order\">\r\n            <p>This product cannot be purchased online. Please call <a href=\"tel:+18668306161\">1-866-830-6161</a> to place your order.</p>\r\n        </div>\r\n        <div class=\"productView-options\">\r\n\r\n\r\n            <form class=\"form\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\"\r\n                  data-cart-item-add>\r\n                <input type=\"hidden\" name=\"action\" value=\"add\">\r\n                <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\r\n\r\n                    {{#each product.customizations}}\r\n                        {{{dynamicComponent 'components/products/customizations'}}}\r\n                    {{/each}}\r\n\r\n                <div data-product-option-change>\r\n                    {{#each product.options}}\r\n                        {{{dynamicComponent 'components/products/options'}}}\r\n                    {{/each}}\r\n                </div>\r\n\r\n                <div class=\"form-field form-field--stock{{#unless product.stock_level}} u-hiddenVisually{{/unless}}\">\r\n\r\n                    <label class=\"form-label form-label--alternate\">\r\n                        {{lang 'products.current_stock'}}\r\n                        <span data-product-stock>{{product.stock_level}}</span>\r\n                    </label>\r\n\r\n                </div>\r\n\r\n                {{#if product.can_purchase}}\r\n\r\n                    <div class=\"form-field form-field--increments\">\r\n                        <label class=\"form-label form-label--alternate\"\r\n                               for=\"qty[]\">QTY</label>\r\n\r\n                        <div class=\"form-increment\" data-quantity-change>\r\n                            <button class=\"button button--icon\" data-action=\"dec\">\r\n                                <span class=\"u-hiddenVisually\">{{lang 'products.quantity_decrease'}}</span>\r\n                                <i class=\"icon\" aria-hidden=\"true\">\r\n                                    <svg>\r\n                                        <use xlink:href=\"#icon-keyboard-arrow-down\"/>\r\n                                    </svg>\r\n                                </i>\r\n                            </button>\r\n                            <input class=\"form-input form-input--incrementTotal\"\r\n                                   id=\"qty[]\"\r\n                                   name=\"qty[]\"\r\n                                   type=\"tel\"\r\n                                   value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\r\n                                   data-quantity-min=\"{{product.min_purchase_quantity}}\"\r\n                                   data-quantity-max=\"{{product.max_purchase_quantity}}\"\r\n                                   min=\"1\"\r\n                                   pattern=\"[0-9]*\"\r\n                                   aria-live=\"polite\">\r\n                            <button class=\"button button--icon\" data-action=\"inc\">\r\n                                <span class=\"u-hiddenVisually\">{{lang 'products.quantity_increase'}}</span>\r\n                                <i class=\"icon\" aria-hidden=\"true\">\r\n                                    <svg>\r\n                                        <use xlink:href=\"#icon-keyboard-arrow-up\"/>\r\n                                    </svg>\r\n                                </i>\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div class=\"alertBox productAttributes-message\" style=\"display:none\">\r\n                        <div class=\"alertBox-column alertBox-icon\">\r\n                            <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n                        </div>\r\n                        <p class=\"alertBox-column alertBox-message\"></p>\r\n                    </div>\r\n\r\n                    <div class=\"form-action atc-btn\">\r\n                        <input id=\"form-action-addToCart\" data-wait-message=\"ADD TO BAG\" class=\"button button--primary\" type=\"submit\"\r\n                               value=\"ADD TO BAG\">\r\n                    </div>\r\n\r\n                    <div class='pdRewardsPromoMessage' productId='{{product.id}}'></div>\r\n\r\n                  \r\n\r\n                {{else}}\r\n\r\n                    {{#any product.custom_fields name=\"discontinued\"}}\r\n                    {{>components/common/alert-error (lang 'products.discontinued')}}\r\n                    \r\n                    {{else}}  \r\n                        {{#if product.out_of_stock_message}}\r\n                            {{>components/common/alert-error product.out_of_stock_message}}\r\n                        {{else}}\r\n                            {{>components/common/alert-error (lang 'products.sold_out')}}\r\n                        {{/if}}\r\n                    {{/any}}\r\n\r\n                {{/if}}\r\n                \r\n                {{{snippet 'product_addtocart'}}}\r\n            </form>\r\n            \r\n        </div>\r\n\r\n        {{>components/common/share}}\r\n        <div class='yotpo QABottomLine' data-appkey='kj8REvvTdnXvtBjLDKBFI8bToUW1wpAm6N3S6fj5' data-product-id='{{product.id}}'></div>\r\n\r\n        {{{snippet 'product_details'}}}\r\n    </section>\r\n\r\n    <div class=\"product-details-accordion cf\">\r\n        \r\n        <ul>\r\n            <li class=\"open-accordion cf description\">\r\n                <h3><span class=\"open-close\">+</span>DESCRIPTION</h3>\r\n                <span class=\"product-accordion-content\">\r\n                    {{{product.description}}}\r\n                    {{{snippet 'product_description'}}}\r\n                </span>\r\n            </li>\r\n            <li class=\"cf at-a-glance\">\r\n                <h3><span class=\"open-close\">+</span>AT A GLANCE</h3>\r\n                <span class=\"product-accordion-content\">\r\n                    <ul>\r\n                    {{#each product.custom_fields}}\r\n                        <span class=\"{{name}}\">\r\n                            <li class=\"productView-info-name {{name}}\">{{name}}:</li>\r\n                            <li class=\"productView-info-value {{{value}}}\">{{{value}}}</li>\r\n                        </span>\r\n                    {{/each}}\r\n                    </ul>\r\n                </span>\r\n            </li>\r\n            <li class=\"cf how-to-use\">\r\n                <h3><span class=\"open-close\">+</span>HOW TO USE</h3>\r\n                <span class=\"product-accordion-content\">\r\n                    <p></p>\r\n                </span>\r\n            </li>\r\n             <li class=\"cf ingredients\">\r\n                <h3><span class=\"open-close\">+</span>INGREDIENTS</h3>\r\n                <span class=\"product-accordion-content\">\r\n                    <p></p>\r\n                </span>\r\n            </li>\r\n        </ul>\r\n\r\n    </div>\r\n\r\n    <div class=\"pp-olapic\">\r\n\r\n        <!-- NEW OLAPIC 1/3/16 -->\r\n        <div id=\"olapic_specific_widget\"></div><script type=\"text/javascript\" src=\"https://photorankstatics-a.akamaihd.net/81b03e40475846d5883661ff57b34ece/static/frontend/latest/build.min.js\"  data-olapic=\"olapic_specific_widget\" data-instance=\"84a8f1748ee70bd40152aa83f7aaf9c7\" data-apikey=\"c8d32af7a5848379872b931655c849bffa25d283a9d4b300435aa1dfd8aa18f1\" data-tags=\"{{product.sku}}\" async=\"async\"></script>\r\n        <!-- Please place the dynamic variable for you product unique id into the 'data-tags' attribute in our javascript -->\r\n        \r\n    </div>\r\n\r\n\r\n</div>\r\n\r\n<div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\r\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n        <span aria-hidden=\"true\">&#215;</span>\r\n    </a>\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n\r\n<script>\r\n\r\n    var slug = window.location.pathname;\r\n\r\n    dataLayer = [{\r\n      'pageType': 'product',\r\n      'productId': '{{product.id}}',\r\n      'productSKU': '{{product.sku}}',\r\n      'pageTitle': '{{product.title}}',\r\n      'imageUrl': '{{getImage product.main_image 'gallery'}}',\r\n      'price': '{{product.price.without_tax.value}}',\r\n      'slug': slug,\r\n      'brandName': '{{product.brand.name}}',\r\n      'email': '{{customer.email}}'\r\n    }];\r\n    console.log(dataLayer);\r\n\r\n</script>\r\n\r\n<script type=\"text/javascript\">\r\n\r\n    var sidecar = sidecar || {};\r\n\r\n    sidecar.product_info = {\r\n        product_id: '{{product.id}}'\r\n    };\r\n\r\n</script>"}
