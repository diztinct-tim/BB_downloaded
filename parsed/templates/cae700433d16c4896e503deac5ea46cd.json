{"components/products/card-no-yotpo":"<article class=\"card {{#if alternate}}card--alternate{{/if}}\">\r\n    <figure class=\"card-figure\">\r\n\r\n        <p>\r\n            {{#each product.custom_fields}}\r\n                <dt class=\"productView-info-name\">{{name}}:</dt>\r\n                <dd class=\"productView-info-value\">{{{value}}}</dd>\r\n            {{/each}}\r\n        </p>\r\n\r\n        <a href=\"{{url}}\">\r\n            <img class=\"card-image\" src=\"{{getImage image 'gallery' (cdn theme_settings.default_image_product)}}\" alt=\"{{image.alt}}\">\r\n        </a>\r\n        <figcaption class=\"card-figcaption {{#if brand.name '==' 'Philosophy'}}no-atc{{/if}}\">\r\n            <div class=\"card-figcaption-body\">\r\n                \r\n                {{#if show_cart_action}}\r\n\r\n                    {{#if has_options}}\r\n                        <a href=\"{{url}}\" class=\"button button--small card-figcaption-button\" data-product-id=\"{{id}}\">VIEW OPTIONS</a>\r\n                    {{/if}}\r\n\r\n                    {{#if add_to_cart_url }}\r\n                        <a href=\"{{add_to_cart_url}}\" class=\"button button--small card-figcaption-button\">ADD TO BAG</a>\r\n                    {{/if}}\r\n\r\n                    {{#if out_of_stock_message }}\r\n                        <a href=\"{{url}}\" class=\"button button--small card-figcaption-button\" data-product-id=\"{{id}}\">OUT OF STOCK</a>\r\n                    {{/if}} \r\n\r\n                {{/if}}\r\n\r\n            </div>\r\n        </figcaption>\r\n    </figure>\r\n    <div class=\"card-body\">\r\n        \r\n        <p class=\"card-text brand-name\" data-test-info-type=\"brandName\">{{brand.name}}</p>\r\n        \r\n        <h4 class=\"card-title\">\r\n            <a href=\"{{url}}\">{{name}}</a>\r\n        </h4>\r\n\r\n        <div class=\"card-text\" data-test-info-type=\"price\">\r\n            {{> components/products/price price=price}}\r\n        </div>\r\n\r\n        {{#if custom_fields}}\r\n        <ul class=\"hide-cfs\">\r\n        {{#each custom_fields}}\r\n            <li class=\"custom-field {{name}}\">\r\n                <span class=\"productView-info-name {{name}}\">{{name}}:</span>\r\n                <span class=\"productView-info-value {{{value}}}\">{{{value}}}</span>\r\n            </li>\r\n        {{/each}}\r\n        </ul>    \r\n        {{/if}}\r\n\r\n        <a href=\"{{url}}\" class=\"po-wrap\">\r\n            <div class=\"yotpo bottomLine\"\r\n            data-product-id=\"{{id}}\"\r\n            data-url=\"{{url}}\">\r\n            </div>\r\n        </a>\r\n\r\n    </div>\r\n</article>\r\n","components/products/carousel":"<section class=\"productCarousel\"\r\n    data-slick='{\r\n        \"dots\": true,\r\n        \"infinite\": false,\r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 2,\r\n        \"slidesToScroll\": 2,\r\n        \"responsive\": [\r\n            {\r\n                \"breakpoint\": 1260,\r\n                \"settings\": {\r\n                    \"slidesToScroll\": 3,\r\n                    \"slidesToShow\": 4\r\n                }\r\n            },\r\n            {\r\n                \"breakpoint\": 800,\r\n                \"settings\": {\r\n                    \"slidesToScroll\": 3,\r\n                    \"slidesToShow\": 4\r\n                }\r\n            },\r\n            {\r\n                \"breakpoint\": 550,\r\n                \"settings\": {\r\n                    \"slidesToScroll\": 3,\r\n                    \"slidesToShow\": 3\r\n                }\r\n            }\r\n        ]\r\n    }'\r\n>\r\n\r\n    {{#each products}}\r\n    <div class=\"productCarousel-slide\">\r\n        {{> components/products/card-no-yotpo }}\r\n    </div>\r\n    {{/each}}\r\n\r\n</section>\r\n","components/products/price":"<!-- {{#if price.with_tax}}\r\n    <div class=\"price-section price-section--withTax\" {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n        {{#if price.rrp_with_tax}}\r\n            <span data-product-rrp-with-tax class=\"price price--rrp\">{{price.rrp_with_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span data-product-price-with-tax class=\"price\">{{price.with_tax.formatted}}</span>\r\n        {{#if price.without_tax}}\r\n            {{lang 'products.price_with_tax' tax_label=price.tax_label}}\r\n        {{/if}}\r\n    </div>\r\n{{/if}}\r\n\r\n{{#if price.without_tax}} -->\r\n    <div class=\"price-section price-section--withoutTax {{#if price.with_tax}}price-section--minor{{/if}}\"  {{#if schema_org}}itemprop=\"offers\" itemscope itemtype=\"http://schema.org/Offer\"{{/if}}>\r\n\r\n        {{#if schema_org}}\r\n            <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\">\r\n            <meta itemprop=\"priceCurrency\" content=\"{{currency_selector.active_currency_code}}\">\r\n        {{/if}}\r\n        <span data-product-price-without-tax class=\"price price--withoutTax\">{{price.without_tax.formatted}}</span>\r\n        {{#if price.with_tax}}\r\n            <abbr title=\"{{lang 'products.excluding_tax'}}\">{{lang 'products.price_without_tax' tax_label=price.tax_label}}</abbr>\r\n        {{/if}}\r\n        {{#if price.rrp_without_tax}}\r\n            <span data-product-rrp-without-tax class=\"price price--rrp retail\">{{price.rrp_without_tax.formatted}}</span>\r\n        {{/if}}\r\n        <span class=\"percent-off\"></span>\r\n    </div>\r\n<!-- {{/if}} -->\r\n\r\n<!-- {{#if page_type '===' 'product'}}\r\n    {{#if price.saved}}\r\n        <div class=\"price-section price-section--saving\">\r\n            <span class=\"price\">\r\n                {{lang 'products.you_save' amount=price.saved.formatted}}\r\n            </span>\r\n        </div>\r\n    {{/if}}\r\n{{/if}}\r\n -->"}
